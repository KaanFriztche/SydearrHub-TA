-- Create Tab 3
local Tab4 = Window:MakeTab({
    Name = "Teleport",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- Add a section to Tab 3
local Section4 = Tab4:AddSection({
    Name = "- Teleport Bölgeleri -"
})

-- Add TSK BASE button to Tab 3
Tab4:AddButton({
    Name = "TSK Base",
    Callback = function()
        local teleportLocations = {
            MetroDoor = Vector3.new(3077.62, 34.01, -252.62)
        }

        -- Karakter ve HumanoidRootPart'ı kontrol et
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

        local function teleportTo(locationName)
            local destination = teleportLocations[locationName]
            if destination then
                humanoidRootPart.CFrame = CFrame.new(destination)
            else
                warn("Geçersiz konum: " .. tostring(locationName))
            end
        end

        teleportTo("MetroDoor")
    end
})

-- Add TSK BASE button to Tab 3
Tab4:AddButton({
    Name = "Denetim Alanı",
    Callback = function()
        local teleportLocations = {
            MetroDoor1 = Vector3.new(1143.87, 13.76, -253.43)
        }

        -- Karakter ve HumanoidRootPart'ı kontrol et
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

