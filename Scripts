local player = game.Players.LocalPlayer
local OrionLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/KaanFriztche/SydearrHubLibrary/refs/heads/main/newfile.lua'))()

local Window = OrionLib:MakeWindow({
    Name = "Sydearr Hub - TA  v1.3",
    HidePremium = false,
    SaveConfig = true,
    ConfigFolder = "OrionTest"
})

-- Create a tab
local Tab1 = Window:MakeTab({
    Name = "Anasayfa",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- Add a section to the tab
local Section1 = Tab1:AddSection({
    Name = "- GUI Anasayfası -"
})

Tab1:AddParagraph("Sydearr Hub Sunar:", [[
 
- Yapılan ilk TA GUI hilesidir.
 
- Sunucumuza gelip GUI hakkında isteklerinizi belirtebilirsiniz.
 
- Discord sunucumuzda mümkün olduğunca güncelleme isteyebilirsiniz ama güncelleme'nin ne hakkında olması gerektiğini bildirmeniz gerekir.
 
- Teleport bölgeleri, sizin yakalanma ihtimalinizi en minimuma indirecek şekilde metro bölgeleri olarak ayarlandı.
]])


Tab1:AddParagraph("Discord Sunucularımız:", [[

- Ana Sunucumuz: https://discord.gg/xTafcNjWcd

- Script Sunucumuz: https://discord.gg/AR2AeEnPDG
]])

Tab1:AddParagraph("GUI Developers:", [[
 
- @kaansfriztche

- @ross_x1
]])

Tab1:AddParagraph("Content Finders:", [[

- @ross_x1 

- @rip_sigma_31

- @rewaqz

- @kaansfriztche
]])


-- Create a tab
local Tab2 = Window:MakeTab({
    Name = "JJ ÇEK",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- Add a section to the tab
local Section2 = Tab2:AddSection({
    Name = "- Jump Jack Toolları -"
})

-- Create a notification
OrionLib:MakeNotification({
    Name = "Hoşgeldin!",
    Content = "Sydearr Hub'a Hoşgeldin!",
    Image = "rbxassetid://4483345998",
    Time = 8
})




Tab2:AddButton({
    Name = "• 30. JJ ÇEK!",
    Callback = function()
        local player = game.Players.LocalPlayer
        local backpack = player:FindFirstChild("Backpack")
        local character = player.Character or player.CharacterAdded:Wait()

        if not backpack or not character then
            warn("Backpack veya Character bulunamadı!")
            return
        end

        local toolName = "30. JJ Çek"

        -- Eğer tool zaten varsa, tekrar oluşturma
        for _, item in ipairs(backpack:GetChildren()) do
            if item:IsA("Tool") and item.Name == toolName then
                print("Tool zaten var, tekrar eklenmeyecek.")
                return
            end
        end

        -- Yeni Tool oluştur
        local tool = Instance.new("Tool")
        tool.Name = toolName
        tool.RequiresHandle = false
        tool.Parent = backpack

        -- Mesaj Listesi
        local messages = {
            "BİR", "İKİ", "ÜÇ", "DÖRT", "BEŞ",
            "ALTI", "YEDİ", "SEKİZ", "DOKUZ", "ON",
            "ON BİR", "ON İKİ", "ON ÜÇ", "ON DÖRT", "ON BEŞ",
            "ON ALTı", "ON YEDİ", "ON SEKİZ", "ON DOKUZ", "YİRMİ",
            "YİRMİ BİR", "YİRMİ İKİ", "YİRMİ ÜÇ", "YİRMİ DÖRT", "YİRMİ BEŞ",
            "YİRMİ ALTI", "YİRMİ YEDİ", "YİRMİ SEKİZ", "YİRMİ DOKUZ", "OTUZ"
        }

        local interval = 2 -- Mesajlar arası toplam süre (saniye)
        local isActive = false -- Tekrar kullanım engelleme

        -- **Zıplama işlemini doğrudan karaktere bağlayan fonksiyon**
        local function forceJump()
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid:ChangeState(Enum.HumanoidStateType.Jumping) -- **Kesin zıplatır**
            else
                warn("Humanoid bulunamadı!")
            end
        end

        -- **Mesaj ve Zıplama Fonksiyonu**
        local function sendMessages()
            if isActive then return end
            isActive = true

            local chatService = game:GetService("TextChatService")
            local channel = chatService.TextChannels.RBXGeneral

            if channel then
                for i, message in ipairs(messages) do
                    -- 20. mesaja gelindiğinde 10 saniye bekle
                    if i == 20 then
                        task.wait(10)
                    end
                    channel:SendAsync(message) -- Mesaj gönder
                    task.wait(0.5) -- 0.5 saniye bekle
                    forceJump() -- **Yeni zıplama fonksiyonunu çağır**
                    task.wait(interval - 0.5) -- Kalan süreyi bekle
                end
            else
                warn("Chat kanalı bulunamadı!")
            end

            isActive = false
        end

        -- **Tool Kullanıldığında Mesaj Gönderme**
        tool.Activated:Connect(sendMessages)
    end
})






