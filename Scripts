-- Türkçe sayıları yazıya çeviren fonksiyon
local function numberToTurkishText(number)
    local numbersInTurkish = {
        "Bir", "İki", "Üç", "Dört", "Beş",
        "Altı", "Yedi", "Sekiz", "Dokuz", "On",
        "On Bir", "On İki", "On Üç", "On Dört", "On Beş",
        "On Altı", "On Yedi", "On Sekiz", "On Dokuz", "Yirmi",
        "Yirmi Bir", "Yirmi İki", "Yirmi Üç", "Yirmi Dört", "Yirmi Beş",
        "Yirmi Altı", "Yirmi Yedi", "Yirmi Sekiz", "Yirmi Dokuz", "Otuz",
        "Otuz Bir", "Otuz İki", "Otuz Üç", "Otuz Dört", "Otuz Beş",
        "Otuz Altı", "Otuz Yedi", "Otuz Sekiz", "Otuz Dokuz", "Kırk",
        "Kırk Bir", "Kırk İki", "Kırk Üç", "Kırk Dört", "Kırk Beş",
        "Kırk Altı", "Kırk Yedi", "Kırk Sekiz", "Kırk Dokuz", "Elli"
    }
    return numbersInTurkish[number] or tostring(number) -- Tablo dışında bir sayı gelirse olduğu gibi döner
end

-- GJ mesajlarını gönderen ana fonksiyon
local function sendGJMessages(count)
    -- `DefaultChatSystemChatEvents` kontrolü
    local replicatedStorage = game:GetService("ReplicatedStorage")
    local chatEvent = replicatedStorage:FindFirstChild("DefaultChatSystemChatEvents")
    if not chatEvent or not chatEvent:FindFirstChild("SayMessageRequest") then
        warn("Mesaj gönderme servisi bulunamadı! Mesajlar gönderilemiyor.")
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "Hata!",
            Text = "Mesaj gönderilemedi, oyun izin vermiyor.",
            Duration = 5
        })
        return
    end

    -- Mesajları sırayla gönder
    for i = 1, count do
        local message = numberToTurkishText(i) .. "." -- Mesaj formatı: "Bir.", "İki.", vb.
        local success, errorMessage = pcall(function()
            chatEvent.SayMessageRequest:FireServer(message, "All") -- Mesaj gönderimi
        end)

        -- Hata kontrolü
        if not success then
            warn("Mesaj gönderilemedi: " .. (errorMessage or "Bilinmeyen hata"))
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "Hata!",
                Text = "Mesaj gönderilemedi: " .. (errorMessage or "Bilinmeyen hata"),
                Duration = 5
            })
        end

        -- Her mesaj arasında bekleme süresi
        task.wait(2)
    end
end

-- GUI butonları
Tab3:AddButton({
    Name = "• 20 GJ ÇEK!",
    Callback = function()
        sendGJMessages(20) -- 20 mesaj gönderir
    end
})

Tab3:AddButton({
    Name = "• 30 GJ ÇEK!",
    Callback = function()
        sendGJMessages(30) -- 30 mesaj gönderir
    end
})

Tab3:AddButton({
    Name = "• 40 GJ ÇEK!",
    Callback = function()
        sendGJMessages(40) -- 40 mesaj gönderir
    end
})

Tab3:AddButton({
    Name = "• 50 GJ ÇEK!",
    Callback = function()
        sendGJMessages(50) -- 50 mesaj gönderir
    end
})




